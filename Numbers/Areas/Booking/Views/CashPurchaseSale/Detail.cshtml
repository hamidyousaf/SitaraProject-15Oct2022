@model Numbers.Entity.Models.BkgVehicleIGP
@{
    ViewData["Title"] = "Booking Vehicle IGP Detail";
    ViewData["CurrentPage"] = "Vehicle IGPs";

}

<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Booking Vehicle IGP Detail</h5>

            </div>
            <div class="ibox-content">
                <form class="form-horizontal">

                    <div class="form-group">
                        <label asp-for="IGPDate" class="col-lg-2 control-label"></label>
                        <div class="col-lg-10">
                            @Html.DisplayFor(model => model.IGPDate)
                        </div>
                    </div>

                    <div class="form-group">
                        <label asp-for="IGPNo" class="col-lg-2 control-label"></label>
                        <div class="col-lg-10">
                            @Html.DisplayFor(model => model.IGPNo)
                        </div>
                    </div>
                    <div class="form-group">
                        <label asp-for="VehicleId" class="col-lg-2 control-label"></label>
                        <div class="col-lg-10">
                            @Html.DisplayFor(model => model.Vehicle.Item.Name)
                        </div>
                    </div>

                    <div class="form-group">
                        <label asp-for="EngineNo" class="col-lg-2 control-label"></label>
                        <div class="col-lg-10">

                            @Html.DisplayFor(model => model.EngineNo)
                        </div>
                    </div>
                    <div class="form-group">
                        <label asp-for="ChassisNo" class="col-lg-2 control-label"></label>
                        <div class="col-lg-10">

                            @Html.DisplayFor(model => model.ChassisNo)
                        </div>
                    </div>
                    <div class="form-group">
                        <label asp-for="InvoiceNo" class="col-lg-2 control-label"></label>
                        <div class="col-lg-10">

                            @Html.DisplayFor(model => model.InvoiceNo)
                        </div>
                    </div>
                    <div class="form-group">
                        <label asp-for="DeliveryCommission" class="col-lg-2 control-label"></label>
                        <div class="col-lg-10">

                            @Html.DisplayFor(model => model.DeliveryCommission)
                        </div>
                    </div>
                    <div class="form-group">
                        <label asp-for="Description" class="col-lg-2 control-label"></label>
                        <div class="col-lg-10">
                            @Html.DisplayFor(model => model.Description)
                        </div>
                    </div>

                    <div class="form-group">
                        <label asp-for="Status" class="col-lg-2 control-label"></label>
                        <div class="col-lg-10">
                            @Html.DisplayFor(model => model.Status)
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-4 col-lg-offset-2">
                            <input type="submit" value="Back" class="btn btn-white" asp-action="VehicleIGPList" />
                            <input type="submit" value="Edit" class="btn btn-primary" asp-action="VehicleIGPCreate" asp-route-id="@Model.Id" />
                        </div>
                    </div>
                </form>
            </div>
        </div>

    </div>
</div>

